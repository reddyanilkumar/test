In [4]:

merged_inner = pd.merge(df_student,df_train,on='Student_ID')

In [17]:

merged_inner1 = pd.merge(left=merged_inner,right=df_internship,left_on='Internship_ID', right_on='Internship_ID')

In [18]:

# Get basic information and print it.
n_samples = merged_inner1.shape[0]
n_features = merged_inner1.shape[1]
print "Number of training samples: {}".format(n_samples)
print "Number of features: {}".format(n_features)

