In [105]:



merged_inner1 = merged_inner1[merged_inner1.solved_status != 'UK']




In [106]:



def transform_solved_status(ss):
    if (ss == 'SO'):
        return 1
    if (ss == 'AT'):
        return 0




In [107]:



merged_inner1['solved_status'] = merged_inner1['solved_status'].map(transform_solved_status)




