In [112]:



def transform_level(l):
    if(l == 'E'):
        return 0
    if(l == 'M'):
        return 1
    if(l == 'H'):
        return 2
    if(l == 'E-M'):
        return 3
    if(l == 'M-H'):
        return 4
    




In [113]:



merged_inner1['level'] = merged_inner1['level'].map(transform_level)




